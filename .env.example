# .env.example: Example environment variables for the backend.
# Copy this file to .env and fill in the values for your environment.

# Flask settings
FLASK_APP=app.py
FLASK_ENV=development # 'development' or 'production'

# LLM Configuration
# Use 'local' for the stubbed development client, or 'gemini' for the production client.
BACKEND_LLM=local

# Gemini API Key (only required if BACKEND_LLM is set to 'gemini')
GEMINI_API_KEY="YOUR_GEMINI_API_KEY_HERE"
GEMINI_API_ENDPOINT="YOUR_GEMINI_ENDPOINT_HERE"
